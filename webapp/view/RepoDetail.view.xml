<mvc:View
	controllerName="githubapiui5.controller.RepoDetail"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0">

    <Page showNavButton="true" navButtonPress=".onNavBack">
        <ObjectHeader title="{repos>name}">
            <ObjectAttribute text="{repos>description}"/>
        </ObjectHeader>
    </Page>

</mvc:View>